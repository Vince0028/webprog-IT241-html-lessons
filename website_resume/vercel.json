{
  "buildCommand": "bash config/vercel-build.sh",
  "outputDirectory": "public",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/resume",
      "destination": "/index.html"
    },
    {
      "source": "/",
      "destination": "/simple_portfolio.html"
    }
  ]
}